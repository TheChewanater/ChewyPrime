package chewyprime

import com.jme3.scene.control.CameraControl
import com.jme3.renderer.Camera
import com.jme3.scene.*
import com.jme3.input.*
import com.jme3.input.controls.*
import com.jme3.math.*

class ChewyPlayerNode extends Node
{
  private mControl
  
  public ChewyPlayerNode(String name, InputManager inputManager, ChewyCameraNode camera)
  {
    super(name);
    
    mControl = new PlayerControl(inputManager, camera)
    addControl(mControl)
  }
}

